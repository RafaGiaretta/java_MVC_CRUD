<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entities.Cliente" table="tb_cliente">
		<id name="idCliente" type="int" column="idCliente">
			<generator class="native" />
		</id>
		<property name="nome" column="nome" type="string" />
		<property name="endereco" column="endereco" type="string" />
		<property name="cidade" column="cidade" type="string" />
		<property name="fone" column="telefone" type="string" />
	</class>
</hibernate-mapping>
